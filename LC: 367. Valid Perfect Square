LC: 367. Valid Perfect Square

class Solution:
    def isPerfectSquare(self, num: int) -> bool:
        l, r= 0, num

        while l<= r:
            mid= (l+r)//2
            sq= mid*mid 
            
            if sq== num:
                return True
            if sq< num:
                l= mid+1
            if sq > num:
                r= mid-1
        
        return False



  


